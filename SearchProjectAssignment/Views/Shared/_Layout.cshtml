<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Search Application implementation</title>


    <link rel="shortcut icon" href="~/Images/fav.png" />

    <link href="~/Content/font-awesome-4.7.0/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/dataTables.bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-datepicker3.min.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-timepicker.min.css" rel="stylesheet" />
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/buttons.dataTables.css" rel="stylesheet" />


</head>
<body>

    <nav class="navbar navbar-default">
        <div class="container-fluid">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header col-md-1 img">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a style="padding-top:4px" class="navbar-brand" href="#"><img alt="" class="img-responsive" src="~/Content/images/no_image.jpg" style="height:40px" /></a>
            </div>

            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse col-md-offset-1 col-md-4 pull-left userAcc" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav">
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Welcome, Gaurav Kholi <span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li><a href="#"><span><i class="glyphicon glyphicon-user"></i> </span>My Profile</a></li>
                            <li><a href="#"><span><i class="glyphicon glyphicon-inbox"></i> </span>Mail Box </a></li>
                            <li><a href="#"><span><i class="glyphicon glyphicon-wrench"></i> </span>Settings </a></li>
                            <li role="separator" class="divider"></li>
                            <li><a href="#"><span><i class="glyphicon glyphicon-pencil"></i></span> Change Password </a></li>
                            <li role="separator" class="divider"></li>
                            <li><a href="#"><span><i class="glyphicon glyphicon-log-out"></i> </span>Log Out </a></li>
                        </ul>
                    </li>
                </ul>
                <form class="navbar-form navbar-left">
                    <div class="form-group has-feedback">
                        <input type="text" class="form-control textBox" placeholder=""> <span class="glyphicon glyphicon-search form-control-feedback"></span>
                    </div>
                </form>

            </div><!-- /.navbar-collapse -->

            <div class="collapse navbar-collapse col-md-5 pull-right" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav">
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Fox <span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li><a href="#">Fox</a></li>
                            <li><a href="#">Fox action</a></li>
                            <li><a href="#">Fox Something else here</a></li>
                            <li role="separator" class="divider"></li>
                            <li><a href="#">Fox Separated link</a></li>
                            <li role="separator" class="divider"></li>
                            <li><a href="#">Fox One more separated link</a></li>
                        </ul>
                    </li>
                </ul>
                <form class="navbar-form navbar-left well topLabel3">
                    <div class="form-group">
                        <input type="text" value="In Fleet 81" class="form-control-static topLabel">
                        <input type="text" value="Available 48" class="form-control-static topLabel">
                        <input type="text" value="Rented Out 26" class="form-control-static topLabel2">
                    </div>
                </form>

            </div><!-- /.navbar-collapse -->
        </div><!-- /.container-fluid -->
    </nav>





    @RenderBody()
    <hr />
    <footer>
        <p>&copy; @DateTime.Now.Year - Search Application implementation</p>
    </footer>


    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script type="text/javascript" src="/scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/jquery.dataTables.min.js"></script>
    <script src="~/Scripts/dataTables.bootstrap.min.js"></script>
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
    <script src="~/Scripts/vash.min.js"></script>
    <script type="text/javascript" src="/scripts/moment.min.js"></script>
    <script src="~/Scripts/bootstrap-datepicker.min.js"></script>
    <script src="~/Scripts/bootstrap-timepicker.min.js"></script>
    <script src="~/Scripts/dataTables.buttons.js"></script>
    <script src="~/Scripts/Search.js"></script>

    @RenderSection("scripts", required: false)

</body>
</html>